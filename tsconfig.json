{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "rootDir": "./src/",
    "baseUrl": "./src",
    "outDir": "./dist/",
    "strict": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "typeRoots": ["node_modules/@types"],
    "sourceMap": false,
    "incremental": true,
    "paths": {
      "@/*": ["./*"],
      "@src/*": ["./*"],
      "@config/*": ["./config/*"],
      "@commands/*": ["./commands/*"],
      "@events/*": ["./events/*"],
      "@entities/*": ["./entities/*"],
      "@utils/*": ["./utils/*"],
      "@service/*": ["./service/*"]
    }
  },
  "files": ["src/Bot.ts"],
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
